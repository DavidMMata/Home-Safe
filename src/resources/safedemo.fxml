<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Line?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="HomeSafe"
            prefHeight="625.0" prefWidth="800.0"
            style="-fx-background-color: white"
            fx:id="pane">
    <children>
        <Rectangle fx:id="safe" fill="gray" height="500.0" layoutX="50.0" layoutY="50.0" width="700.0" />
        <Rectangle fx:id="door" fill="gray" stroke="black" height="400.0" layoutX="100.0" layoutY="100.0" width="445.0" />
        <Rectangle fx:id="handle" fill="gray" stroke="black" height="100.0" layoutX="150.0" layoutY="250.0" width="50.0" arcHeight="75.0" arcWidth="75.0"/>

        <Rectangle fx:id="doorInside1" visible="false" fill="gray" stroke="black" height="200.0" layoutX="200.0" layoutY="200.0" width="345.0" />
        <Line fx:id="doorInside2" visible="false" stroke="black" startX="100.0" startY="100.0" endX="200.0" endY="200.0" />
        <Line fx:id="doorInside3" visible="false" stroke="black" startX="100.0" startY="500.0" endX="200.0" endY="400.0" />
        <Rectangle fx:id="openDoor" visible="false" fill="gray" stroke="black" height="450.0" layoutX="445.0" layoutY="75.0" width="100.0" />

        <Circle fx:id="led" fill="gray" stroke="black" radius="10.0" layoutX="682.5" layoutY="170.0" />
        <Circle fx:id="batteryLED" fill="gray" stroke="black" radius="10.0" layoutX="647.5" layoutY="170.0" />

        <Button onAction="#keyClicked" text="0" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="635.0" layoutY="305.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="1" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="600.0" layoutY="270.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="2" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="635.0" layoutY="270.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="3" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="670.0" layoutY="270.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="4" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="600.0" layoutY="235.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="5" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="635.0" layoutY="235.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="6" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="670.0" layoutY="235.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="7" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="600.0" layoutY="200.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="8" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="635.0" layoutY="200.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#keyClicked" text="9" prefWidth="25" prefHeight="25" alignment="CENTER" layoutX="670.0" layoutY="200.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#enterClicked" text="ENTER" prefWidth="60" prefHeight="25" alignment="CENTER" layoutX="670.0" layoutY="305.0" textFill="black" style="-fx-background-color: white" />
        <Button onAction="#clearClicked" text="CLEAR" prefWidth="60" prefHeight="25" alignment="CENTER" layoutX="565.0" layoutY="305.0" textFill="black" style="-fx-background-color: white" />

        <Button onAction="#toggleDoor" fx:id="doorBtn" text="Open Door" prefWidth="100" prefHeight="25" alignment="CENTER" layoutX="75.0" layoutY="575.0" textFill="black" style="-fx-background-color: white; -fx-border-color: black;" />

    </children>

</AnchorPane>
